use AppleScript version "2.4" -- Yosemite (10.10) or lateruse scripting additionsset library_dir to path to library folder from user domainset caches_dir to (library_dir & "Caches" as string) as aliason create_export_dir()	my remove_export_dir()		tell application "Finder"		global export_dir		global caches_dir		set export_dir to make new folder at caches_dir with properties {name:"Exported Images"}		-- open export_dir	end tellend create_export_diron remove_export_dir()	tell application "Finder"		global caches_dir		set export_dir to (caches_dir & "Exported Images") as string				if exists export_dir then			beep			beep			delete export_dir -- move the "Exported Images" to the trash		end if	end tellend remove_export_diron create_tags(_rawTags)	set origDelimiters to AppleScript's text item delimiters	set AppleScript's text item delimiters to ","	set theTags to every text item of _rawTags	set AppleScript's text item delimiters to origDelimiters	return theTagsend create_tagstell application "Photos"	set selected_images to selection	if (count of selected_images) > 0 then		my create_export_dir()				global export_dir		export selected_images to (export_dir as alias)				tell application "Finder"			set exported_images to files in export_dir whose name extension is "jpeg"						tell application "Yojimbo"				activate								repeat with curr_image in exported_images					set added_image to import curr_image as alias					tell added_image						set result to display dialog "Enter image name" default answer "Untitled image" buttons {"OK"} ¬							default button 1 with icon note						if text returned of result ≠ "" then							set name to text returned of result						end if												set result to display dialog "Enter Yojimbo tags" default answer "" buttons {"OK"} ¬							default button 1 with icon note						if text returned of result ≠ "" then							set image_tags to my create_tags(text returned of result)							add tags image_tags						end if												reveal					end tell				end repeat			end tell		end tell	end ifend tellmy remove_export_dir()